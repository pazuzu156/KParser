{
    "name": "pazuzu156/kparser",
    "description": "Parsing package for my website",
    "keywords": ["parser", "laravel"],
    "homepage": "http://pazuzu156.github.io/KParser",
    "type": "library",
    "license": "GPL",
    "authors": [
        {
            "name": "Kaleb Klein",
            "email": "klein.jae@gmail.com",
            "homepage": "http://www.kalebklein.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "geshi/geshi": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.0.*"
    },
    "autoload": {
        "psr-0": {"Pazuzu156\\KParser": "src/"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        },
        "laravel": {
            "providers": [
                "Pazuzu156\\KParser\\Laravel\\KParserServiceProvider"
            ],
            "aliases": {
                "KParser": "Pazuzu156\\KParser\\Laravel\\KParserFacade"
            }
        }
    },
    "minimum-stability": "dev"
}
